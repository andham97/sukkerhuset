<!DOCTYPE html>
<html>
<head>
    <!--<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>-->
    <script src="/jQuery.js"></script>
    <script src="/js/booking.js"></script>
    <style>
        table, th, td {
            border: 1px solid rgba(0, 0, 0, 0.2);
        }
        table {
            width: 100%;
        }

        thead tr {
            background: rgb(209, 109, 106);
        }

        tbody tr:nth-child(2n - 1), tbody tr:nth-child(2n ) {
            background: rgb(238, 205, 205);
        }

        tbody tr:nth-child(4n), tbody tr:nth-child(4n - 1) {
            background: rgb(253, 242, 208);
        }

        tbody td[rowspan]:nth-child(1){
            font-size: 20px;
        }

        .menu {
            display: none;
            position: absolute;
            background-color: #f9f9f9;
            min-width: 50px;
            box-shadow: 0px 8px 10px 0px rgba(0,0,0,0.2)
        }

        .menu-item {
            color: black;
            padding: 12px 16px;
            text-decoration: none;
            display: block
        }
    </style>
</head>
<body>
<%- content %>
<div id="kontrakt-menu" class="menu">
    <a href="#" class="menu-item"></a>
    <a href="#" class="menu-item">Avtale</a>
    <a href="#" class="menu-item">Kontrakt sendt</a>
    <a href="#" class="menu-item">Kontrakt signert</a>
</div>
<div id="faktura-menu" class="menu">
    <a href="#" class="menu-item"></a>
    <a href="#" class="menu-item">Sendt</a>
    <a href="#" class="menu-item">Betalt</a>
</div>
</body>
</html>